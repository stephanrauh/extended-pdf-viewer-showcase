{
  "name": "pdf-showcase",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prebuild": "node extract-versions.js",
    "build": "ng build --prod",
    "postbuild": "rm dist/pdf-showcase/assets/pdfs/2gb-sample-file.pdf && rm dist/pdf-showcase/assets/pdfs/us-public-health-and-welfare-code.pdf",
    "test": "ng test",
    "lint": "ng lint",
    "express": "ng b && cd ./dist/pdf-showcase/ && node ./server.js",
    "prestart": "node extract-versions.js",
    "start2": "ng serve --host 0.0.0.0 --disable-host-check",
    "start": "ng serve --configuration development  --base-href /angularapp/ --deploy-url /angularapp/ -o",
    "cypress": "cypress open",
    "quick": "cd ../ngx-extended-pdf-viewer && ./quick.sh && npm run showcase && cd ../extended-pdf-viewer-showcase && ng s --host 0.0.0.0",
    "full": "cd ../ngx-extended-pdf-viewer && ./updateMozillasPdfViewer.sh && npm run showcase && cd ../extended-pdf-viewer-showcase && ng s --host 0.0.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.2",
    "@angular/cdk": "^13.2.2",
    "@angular/common": "^13.2.2",
    "@angular/compiler": "^13.2.2",
    "@angular/core": "^13.2.2",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^13.2.2",
    "@angular/material": "^13.2.2",
    "@angular/platform-browser": "^13.2.2",
    "@angular/platform-browser-dynamic": "^13.2.2",
    "@angular/router": "^13.2.2",
    "@fontsource/material-icons": "^4.5.2",
    "@fontsource/roboto": "^4.5.3",
    "@stephanrauh/angular2-smart-table": "^2.0.9",
    "angular-material-icons": "^0.7.1",
    "classlist.js": "^1.1.20150312",
    "ng2-completer": "^9.0.1",
    "ng2-pdf-viewer": "^8.0.1",
    "ngx-extended-pdf-viewer": "^12.0.1",
    "ngx-markdown": "^13.1.0",
    "ngx-perfect-scrollbar": "^10.1.1",
    "perfect-scrollbar": "^1.5.5",
    "prismjs": "^1.26.0",
    "rxjs": "^7.5.4",
    "tslib": "^2.3.1",
    "web-animations-js": "^2.3.2",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.3",
    "@angular/cli": "^13.2.3",
    "@angular/compiler-cli": "^13.2.2",
    "@angular/language-service": "^13.2.2",
    "@types/node": "^17.0.17",
    "brotli": "^1.3.2",
    "codelyzer": "^6.0.2",
    "cypress": "^9.4.1",
    "helmet": "^5.0.2",
    "node-dir": "^0.1.17",
    "ts-node": "^10.5.0",
    "tslint": "^6.1.3",
    "typescript": "~4.6.3"
  }
}
