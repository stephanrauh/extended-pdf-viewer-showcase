{
  "name": "pdf-showcase",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prebuild": "node extract-versions.js",
    "build": "ng build --configuration production",
    "postbuild": "rm -r dist/pdf-showcase/assets/pdfs/under-copyright",
    "test": "ng test",
    "lint": "ng lint",
    "express": "ng b && cd ./dist/pdf-showcase/ && node ./server.js",
    "prestart": "node extract-versions.js",
    "start2": "ng serve --host 0.0.0.0 --disable-host-check",
    "start": "ng serve --configuration development  --serve-path /angularapp/ -o",
    "cypress": "cypress open",
    "quick": "cd ../ngx-extended-pdf-viewer && ./quick.sh && npm run showcase && cd ../extended-pdf-viewer-showcase && npm run prebuild && ng s --host 0.0.0.0",
    "full": "cd ../ngx-extended-pdf-viewer && ./updateMozillasPdfViewer.sh && npm run showcase && cd ../extended-pdf-viewer-showcase && npm run prebuild && ng s --host 0.0.0.0",
    "ts": "cd ../ngx-extended-pdf-viewer && npm run showcase && cd ../extended-pdf-viewer-showcase && ng s --host 0.0.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.1",
    "@angular/cdk": "^14.2.1",
    "@angular/common": "^14.2.1",
    "@angular/compiler": "^14.2.1",
    "@angular/core": "^14.2.1",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.2.1",
    "@angular/material": "^14.2.1",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-browser-dynamic": "^14.2.1",
    "@angular/router": "^14.2.1",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/roboto": "^4.5.8",
    "angular2-smart-table": "^2.6.0",
    "classlist.js": "^1.1.20150312",
    "ng2-completer": "^9.0.1",
    "ng2-pdf-viewer": "^9.1.2",
    "ngx-extended-pdf-viewer": "^15.0.0",
    "ngx-markdown": "^14.0.1",
    "ngx-perfect-scrollbar": "^10.1.1",
    "perfect-scrollbar": "^1.5.5",
    "prismjs": "^1.29.0",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular/cli": "^14.2.2",
    "@angular/compiler-cli": "^14.2.7",
    "@angular/language-service": "^14.2.1",
    "@types/node": "^18.7.16",
    "brotli": "^1.3.3",
    "codelyzer": "^6.0.2",
    "cypress": "^10.7.0",
    "helmet": "^6.0.0",
    "node-dir": "^0.1.17",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "~4.7.4"
  },
  "overrides": {
    "codelyzer": {
      "@angular/core": ">=14.0.0",
      "@angular/compiler": ">=14.0.0"
    },
    "@angular/flex-layout": {
      "@angular/cdk": ">=14.0.0",
      "@angular/common": ">=14.0.0",
      "@angular/core": ">=14.0.0",
      "@angular/platform-browser": ">=14.0.0"
    },
    "angular2-smart-table": {
      "@angular/cdk": ">=14.0.0",
      "@angular/common": ">=14.0.0",
      "@angular/core": ">=14.0.0",
      "@angular/forms": ">=14.0.0",
      "@angular/platform-browser": ">=14.0.0"
    }
  }
}
