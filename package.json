{
  "name": "pdf-showcase",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "preinstall": "node ./build-tools/check-ignore-scripts.js",
    "install": "npm install --ignore-scripts && npm update --save --ignore-scripts && npm audit fix --ignore-scripts && ./build-tools/search-for-shai-hulud.sh --full && npm rebuild esbuild",
    "update": "npm update --save --ignore-scripts && npm audit fix --ignore-scripts && ./build-tools/search-for-shai-hulud.sh --full && npm rebuild esbuild",
    "prebuild": "node extract-versions.js && rm -rf .angular",
    "build": "ng build --configuration production",
    "start": "ng s --configuration production --open",
    "postbuild": "rm -r -f dist/pdf-showcase/browser/assets/pdfs/under-copyright && rm -f dist/pdf-showcase/browser/stats.json",
    "release": "npm run build && cp ftp/.htaccess dist/pdf-showcase/browser && cd ftp && npx tsx upload.ts",
    "test": "ng test",
    "lint": "ng lint",
    "full": "cd ../ngx-extended-pdf-viewer && npm run build:base && npm run build:lib && cd ../extended-pdf-viewer-showcase && npm run prebuild && ng s --host 0.0.0.0",
    "ts": "cd ../ngx-extended-pdf-viewer && npm run build:lib && cd ../extended-pdf-viewer-showcase && npm run prebuild && ng s --host 0.0.0.0",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/pdf-showcase/browser/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@angular/platform-server": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/ssr": "^21.0.0",
    "@standard-schema/spec": "^1.0.0",
    "angular2-smart-table": "^3.8.0",
    "esbuild": "^0.27.2",
    "marked": "^16.4.2",
    "ngx-extended-pdf-viewer": "^25.6.0",
    "ngx-markdown": "^20.1.0",
    "ngx-scrollbar": "^19.1.1",
    "pdf-lib": "^1.17.1",
    "prismjs": "^1.30.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "web-animations-js": "^2.3.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/language-service": "^21.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.1",
    "angular-eslint": "^20.6.0",
    "autoprefixer": "^10.4.22",
    "basic-ftp": "^5.0.5",
    "brotli": "^1.3.3",
    "dotenv": "^16.6.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^51.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "express": "^5.1.0",
    "glob": "^11.1.0",
    "helmet": "^8.1.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "jest-preset-angular": "^16.0.0",
    "jsdom": "^26.0.0",
    "node-dir": "^0.1.17",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.47.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "overrides": {
    "angular2-smart-table": {
      "@angular/forms": "^21.0.0",
      "@angular/core": "^21.0.0",
      "@angular/common": "^21.0.0"
    },
    "ngx-markdown": {
      "@angular/core": "^21.0.0",
      "@angular/common": "^21.0.0",
      "@angular/platform-browser": "^21.0.0"
    },
    "jest-preset-angular": {
      "@angular/core": "^21.0.0",
      "@angular/compiler-cli": "^21.0.0",
      "@angular-devkit/build-angular": "^21.0.0",
      "@angular/platform-browser-dynamic": "^21.0.0",
      "jest": "^30.0.0"
    }
  }
}
