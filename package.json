{
  "name": "pdf-showcase",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prebuild": "node extract-versions.js",
    "build": "ng build --configuration production",
    "postbuild": "rm -r -f dist/pdf-showcase/browser/assets/pdfs/under-copyright && rm -f dist/pdf-showcase/browser/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "express": "ng b && cd ./dist/pdf-showcase/ && node ./server.js",
    "prestart": "node extract-versions.js",
    "start2": "ng serve --host 0.0.0.0 --disable-host-check",
    "start": "ng serve --configuration development  --serve-path /angularapp/ -o",
    "quick": "cd ../ngx-extended-pdf-viewer && ./quick.sh && npm run showcase && cd ../extended-pdf-viewer-showcase && npm run prebuild && ng s --host 0.0.0.0",
    "full": "cd ../ngx-extended-pdf-viewer && ./updateMozillasPdfViewer.sh && npm run showcase && cd ../extended-pdf-viewer-showcase && npm run prebuild && ng s --host 0.0.0.0",
    "ts": "cd ../ngx-extended-pdf-viewer && npm run showcase && cd ../extended-pdf-viewer-showcase && ng s --host 0.0.0.0",
    "dev:ssr": "ng run pdf-showcase:serve-ssr ",
    "serve:ssr": "node dist/pdf-showcase/server/main.js",
    "build:ssr": "ng build && ng run pdf-showcase:server",
    "full:ssr": "cd ../ngx-extended-pdf-viewer && ./updateMozillasPdfViewer.sh && npm run showcase && cd ../extended-pdf-viewer-showcase && npm run prebuild && npm run dev:ssr",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/pdf-showcase/browser/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.1",
    "@angular/cdk": "^14.2.1",
    "@angular/common": "^15.0.1",
    "@angular/compiler": "^15.0.1",
    "@angular/core": "^15.0.1",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "^15.2.9",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^15.0.1",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/platform-server": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/roboto": "^4.5.8",
    "@nguniversal/express-engine": "^15.2.1",
    "angular2-smart-table": "^2.10.0",
    "classlist.js": "^1.1.20150312",
    "ng2-completer": "^9.0.1",
    "ngx-extended-pdf-viewer": "=18.0.0-alpha.1",
    "ngx-markdown": "^15.1.2",
    "ngx-perfect-scrollbar": "^10.1.1",
    "perfect-scrollbar": "^1.5.5",
    "prismjs": "^1.29.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular/cli": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@nguniversal/builders": "^15.2.1",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.19",
    "brotli": "^1.3.3",
    "codelyzer": "^6.0.2",
    "express": "^4.18.2",
    "helmet": "^6.2.0",
    "node-dir": "^0.1.17",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "~4.8.4",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "overrides": {
    "codelyzer": {
      "@angular/core": ">=14.0.0",
      "@angular/compiler": ">=14.0.0"
    },
    "@angular/flex-layout": {
      "@angular/cdk": ">=14.0.0",
      "@angular/common": ">=14.0.0",
      "@angular/core": ">=14.0.0",
      "@angular/platform-browser": ">=14.0.0"
    },
    "angular2-smart-table": {
      "@angular/cdk": ">=14.0.0",
      "@angular/common": ">=14.0.0",
      "@angular/core": ">=14.0.0",
      "@angular/forms": ">=14.0.0",
      "@angular/platform-browser": ">=14.0.0"
    }
  }
}
