{
  "name": "pdf-showcase",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prebuild": "node extract-versions.js && rm -rf .angular",
    "build": "ng build --configuration production",
    "postbuild": "rm -r -f dist/pdf-showcase/browser/assets/pdfs/under-copyright && rm -f dist/pdf-showcase/browser/stats.json",
    "release": "npm run build && cd ftp && npx tsx upload.ts",
    "test": "ng test",
    "lint": "ng lint",
    "express": "ng b && cd ./dist/pdf-showcase/ && node ./server.js",
    "prestart": "node extract-versions.js",
    "start2": "ng serve --host 0.0.0.0 --disable-host-check",
    "start": "ng serve --configuration development  --serve-path /angularapp/ -o",
    "quick": "cd ../ngx-extended-pdf-viewer && ./quick.sh && npm run showcase && cd ../extended-pdf-viewer-showcase && npm run prebuild && ng s --host 0.0.0.0",
    "full": "cd ../ngx-extended-pdf-viewer && npm run prebuild && ./updateMozillasPdfViewer.sh && npm run showcase && cd ../extended-pdf-viewer-showcase && npm run prebuild && ng s --host 0.0.0.0",
    "ts": "cd ../ngx-extended-pdf-viewer && npm run showcase && cd ../extended-pdf-viewer-showcase && ng s --host 0.0.0.0",
    "dev:ssr": "ng run pdf-showcase:serve-ssr ",
    "serve:ssr": "node dist/pdf-showcase/server/main.js",
    "build:ssr": "ng build && ng run pdf-showcase:server",
    "full:ssr": "cd ../ngx-extended-pdf-viewer && ./updateMozillasPdfViewer.sh && npm run showcase && cd ../extended-pdf-viewer-showcase && npm run prebuild && npm run dev:ssr",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/pdf-showcase/browser/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.4",
    "@angular/cdk": "^16.0.0",
    "@angular/common": "^17.0.4",
    "@angular/compiler": "^17.0.4",
    "@angular/core": "^17.0.4",
    "@angular/forms": "^17.3.10",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^17.0.4",
    "@angular/platform-browser-dynamic": "^17.3.10",
    "@angular/platform-server": "^17.3.10",
    "@angular/router": "^17.3.10",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/roboto": "^4.5.8",
    "angular2-smart-table": "^3.2.0",
    "basic-ftp": "^5.0.5",
    "dotenv": "^16.4.5",
    "ngx-extended-pdf-viewer": "^20.2.0",
    "ngx-markdown": "^17.2.1",
    "ngx-perfect-scrollbar": "^10.1.1",
    "perfect-scrollbar": "^1.5.5",
    "prismjs": "^1.29.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.10",
    "@angular/language-service": "^17.3.10",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.33",
    "brotli": "^1.3.3",
    "codelyzer": "^6.0.2",
    "express": "^4.19.2",
    "helmet": "^6.2.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.6",
    "node-dir": "^0.1.17",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "<5.3.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
