{
  "name": "pdf-showcase",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prebuild": "node extract-versions.js && rm -rf .angular",
    "build": "ng build --configuration production",
    "start": "ng s --configuration production --open",
    "postbuild": "rm -r -f dist/pdf-showcase/browser/assets/pdfs/under-copyright && rm -f dist/pdf-showcase/browser/stats.json",
    "release": "npm run build && cp ftp/.htaccess dist/pdf-showcase/browser && cd ftp && npx tsx upload.ts",
    "test": "ng test",
    "lint": "ng lint",
    "full": "cd ../ngx-extended-pdf-viewer && npm run build:base && npm run build:lib && cd ../extended-pdf-viewer-showcase && npm run prebuild && ng s --host 0.0.0.0",
    "ts": "cd ../ngx-extended-pdf-viewer && npm run build:lib && cd ../extended-pdf-viewer-showcase && npm run prebuild && ng s --host 0.0.0.0",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/pdf-showcase/browser/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.1",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.1.0",
    "@angular/material": "^20.1.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.1.0",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.0",
    "@fontsource/material-icons": "^5.2.5",
    "@fontsource/roboto": "^5.2.6",
    "@material-design-icons/font": "^0.14.15",
    "angular2-smart-table": "^3.7.0",
    "marked": "^15.0.12",
    "ngx-extended-pdf-viewer": "^24.2.0-alpha.0",
    "ngx-markdown": "^20.0.0",
    "ngx-perfect-scrollbar": "^10.1.1",
    "perfect-scrollbar": "^1.5.6",
    "prismjs": "^1.30.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.0",
    "@angular/cli": "^20.1.0",
    "@angular/compiler-cli": "^20.1.0",
    "@angular/language-service": "^20.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.16.3",
    "angular-eslint": "^20.1.1",
    "basic-ftp": "^5.0.5",
    "brotli": "^1.3.3",
    "dotenv": "^16.6.1",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^51.3.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.6.0",
    "node-dir": "^0.1.17",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.36.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "overrides": {
    "angular2-smart-table": {
      "@angular/forms": "^20.0.0",
      "@angular/core": "^20.0.0",
      "@angular/common": "^20.0.0"
    },
    "ngx-markdown": {
      "@angular/core": "^20.0.0",
      "@angular/common": "^20.0.0",
      "@angular/platform-browser": "^20.0.0"
    },
    "jest-preset-angular": {
      "@angular/core": "^20.0.0",
      "@angular/compiler-cli": "^20.0.0",
      "@angular-devkit/build-angular": "^20.0.0",
      "@angular/platform-browser-dynamic": "^20.0.0"
    }
  }
}
