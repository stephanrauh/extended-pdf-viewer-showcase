{
  "name": "pdf-showcase",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prebuild": "node extract-versions.js && rm -rf .angular",
    "build": "ng build --configuration production",
    "postbuild": "rm -r -f dist/pdf-showcase/browser/assets/pdfs/under-copyright && rm -f dist/pdf-showcase/browser/stats.json",
    "release": "npm run build && cp dist/pdf-showcase/browser/extended-pdf-viewer/simple/index.html dist/pdf-showcase/browser && cp ftp/.htaccess dist/pdf-showcase/browser && cd ftp && npx tsx upload.ts",
    "testrelease": "npm run build && cp dist/pdf-showcase/browser/extended-pdf-viewer/simple/index.html dist/pdf-showcase/browser && cp ftp/.htaccess dist/pdf-showcase/browser && cd ftp && npx http-server dist/pdf-showcase/browser",
    "test": "ng test",
    "lint": "ng lint",
    "express": "ng b && cd ./dist/pdf-showcase/ && node ./server.js",
    "prestart": "node extract-versions.js",
    "start2": "ng serve --host 0.0.0.0 --disable-host-check",
    "start": "ng serve --configuration development  --serve-path /angularapp/ -o",
    "quick": "cd ../ngx-extended-pdf-viewer && ./quick.sh && npm run showcase && cd ../extended-pdf-viewer-showcase && npm run prebuild && ng s --host 0.0.0.0",
    "full": "cd ../ngx-extended-pdf-viewer && npm run prebuild && ./updateMozillasPdfViewer.sh && npm run showcase && cd ../extended-pdf-viewer-showcase && npm run prebuild && ng s --host 0.0.0.0",
    "ts": "cd ../ngx-extended-pdf-viewer && npm run showcase && cd ../extended-pdf-viewer-showcase && ng s --host 0.0.0.0",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/pdf-showcase/browser/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.1",
    "@angular/cdk": "^18.2.1",
    "@angular/common": "^18.2.1",
    "@angular/compiler": "^18.2.1",
    "@angular/core": "^18.2.1",
    "@angular/forms": "^18.2.9",
    "@angular/material": "^18.2.10",
    "@angular/platform-browser": "^18.2.1",
    "@angular/platform-browser-dynamic": "^18.2.9",
    "@angular/platform-server": "^18.2.9",
    "@angular/router": "^18.2.9",
    "@angular/ssr": "^18.2.10",
    "@fontsource/material-icons": "^5.1.0",
    "@fontsource/roboto": "^5.1.0",
    "angular2-smart-table": "^3.4.0",
    "basic-ftp": "^5.0.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "ngx-extended-pdf-viewer": "^22.0.0-alpha.3",
    "ngx-markdown": "^18.1.0",
    "ngx-perfect-scrollbar": "^10.1.1",
    "perfect-scrollbar": "^1.5.6",
    "prismjs": "^1.29.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.10",
    "@angular/cli": "^18.2.10",
    "@angular/compiler-cli": "^18.2.9",
    "@angular/language-service": "^18.2.9",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "brotli": "^1.3.3",
    "codelyzer": "^6.0.2",
    "express": "^4.21.1",
    "helmet": "^7.2.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.4",
    "node-dir": "^0.1.17",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "5.5.4",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "overrides": {
    "angular2-smart-table": {
      "@angular/forms": "^18.0.0",
      "@angular/core": "^18.0.0",
      "@angular/common": "^18.0.0"
    }
  }
}
